<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.ApplicationController_UserInteractiveFunctions">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Item">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
            <Label text="Contents of Item" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </top>
         <center>
            <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <content>
                  <TextArea disable="true" prefHeight="200" prefWidth="600" />
               </content></ScrollPane>
         </center></BorderPane>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button onAction="#onBackToHomeClick" mnemonicParsing="false" text="Back to Home" />
        </items>
      </ToolBar>
   </children>


</VBox>
